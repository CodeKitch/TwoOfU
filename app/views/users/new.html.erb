<h1>SIGN UP</h1>

<%= render partial: "layouts/errors", locals: {object: @user} %>


<%= form_for @user, url:'/signup' do |f| %>
<%= f.label :username %>
<%= f.text_field :username %>

<%= f.label :email %>
<%= f.email_field :email, required:true %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.submit "Submit"%>
<% end %>